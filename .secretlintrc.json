{
  "rules": [
    {
      "id": "@secretlint/secretlint-rule-preset-recommend",
      "rules": [
        {
          "id": "@secretlint/secretlint-rule-aws",
          "allowMessageIds": ["AWSAccountID", "AWSAccessKeyID"],
          "options": {
            "allows": ["/DUMMY_/i", "/EXAMPLE_/i", "/TEST_/i", "/PLACEHOLDER_/i"]
          }
        },
        {
          "id": "@secretlint/secretlint-rule-github",
          "allowMessageIds": ["GITHUB_TOKEN"],
          "options": {
            "allows": ["/DUMMY_/i", "/EXAMPLE_/i", "/TEST_/i", "/PLACEHOLDER_/i"]
          }
        },
        {
          "id": "@secretlint/secretlint-rule-slack",
          "allowMessageIds": ["SLACK_TOKEN"],
          "options": {
            "allows": ["/DUMMY_/i", "/EXAMPLE_/i", "/TEST_/i", "/PLACEHOLDER_/i"]
          }
        }
      ]
    }
  ]
}
